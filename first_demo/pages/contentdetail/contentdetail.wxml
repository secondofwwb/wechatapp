
<!--文章详细-->
<view class="container">
  <image class="image_header" src="{{content_key.content_image}}"></image>
  <view class="author_date">
    <image class="avater" src="../../images/icons/math.jpg"></image>
    <text class="author">second</text>
    <text class="const_text">发表于</text>
    <text class="date">time</text>
  </view>
  <text class="title">{{content_key.content_title}}</text>
  <view class="tool">
    <view class="circle-img" >
      <image src="../../images/icons/collection.png"></image>
      <image class="share-img" src="../../images/icons/share.png"></image>
    </view>
    <view class="horizon"></view>
  </view>
  <text class="detail" >
    {{content_key.outline}}
  </text>
</view>